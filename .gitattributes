# Mark Yarn related files
/.yarn/**            linguist-vendored
/.yarn/releases/*    binary
/.yarn/plugins/**/*  binary
/.pnp.*              binary linguist-generated

# Treat fixtures as generated files.
__fixtures__/**      linguist-generated
__fixtures__/**      -diff

# Treat storyblok components as generated files.
storyblok/*.json       linguist-generated
storyblok/*.json       -diff

# Mark yarn.lock file as vendored and not diffable.
yarn.lock linguist-vendored
yarn.lock -diff
